#include <bits/stdc++.h>

using namespace std;

int main()
{
     int k=1556;int c=0;
     
    int n=k%10;
     k=k/10;
     while(k!=0){
        int j=k%10;
        if(j>n)
        {  c=1;
            cout<<"false";
            break;
        }
        n=j;
        k=k/10;
        
     }
     if(c==0)
    cout<<"True ,it is s tidy number";
    return 0;
}
